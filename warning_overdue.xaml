        <Label Content="ISBN:" HorizontalAlignment="Left" Margin="219,208,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label x:Name="isbn_warning" Content="" HorizontalAlignment="Left" Margin="267,208,0,0" VerticalAlignment="Top" FontSize="14"/>
