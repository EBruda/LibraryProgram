        <Label x:Name="title_warning" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="267,297,0,0" FontSize="14" RenderTransformOrigin="1.4,0.552"/>
        <Label Content="Id:" HorizontalAlignment="Left" Margin="232,163,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Title:" HorizontalAlignment="Left" Margin="220,297,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Author:" HorizontalAlignment="Left" Margin="206,343,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.531,0.385" FontSize="14"/>
        <Label Content="Holder:" HorizontalAlignment="Left" Margin="206,386,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Date checked out:" HorizontalAlignment="Left" Margin="136,429,0,0" VerticalAlignment="Top" FontSize="14"/>
